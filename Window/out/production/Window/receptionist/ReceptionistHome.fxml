<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="receptionist.ReceptionistHomeController">
   <children>
      <Label fx:id="lb_name" text="Receptionist" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button fx:id="btn_logout" mnemonicParsing="false" onAction="#pressBtnLogout" text="Logout" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
      <Button fx:id="btn_reservation" mnemonicParsing="false" text="+ Reservation" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="50.0" />
      <Button fx:id="btn_cancel_reservation" mnemonicParsing="false" text="- Cancel reservation" AnchorPane.rightAnchor="125.0" AnchorPane.topAnchor="50.0" />
      <Button fx:id="btn_check_in" mnemonicParsing="false" text="Check in" AnchorPane.leftAnchor="100.0" AnchorPane.topAnchor="50.0" />
      <Button fx:id="btn_check_out" mnemonicParsing="false" text="Check out" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="50.0" />
      <TableView prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="90.0" />
   </children>
</AnchorPane>

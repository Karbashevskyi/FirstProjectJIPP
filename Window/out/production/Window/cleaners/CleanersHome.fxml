<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cleaners.CleanersHomeController">
    <children>
        <Label fx:id="lb_name" text="Cleaners" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0">
            <font>
                <Font size="25.0" />
            </font>
        </Label>
        <Label fx:id="lb_selected_room_id" text="Selected room id:" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="62.5" />
        <Button fx:id="btn_logout" mnemonicParsing="false" onAction="#pressBtnLogout" text="Logout" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
        <Button fx:id="btn_cleaning" mnemonicParsing="false" onAction="#pressBtnCleaning" text="Clearning" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="50.0" />
        <TableView fx:id="table_rooms" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="90.0">
         <columns>
             <TableColumn fx:id="tb_id" prefWidth="75.0" text="Id" />
             <TableColumn fx:id="tb_status" prefWidth="75.0" text="Status" />
             <TableColumn fx:id="tb_client_full_name" prefWidth="75.0" text="Client" />
         </columns>
        </TableView>
    </children>
</AnchorPane>
